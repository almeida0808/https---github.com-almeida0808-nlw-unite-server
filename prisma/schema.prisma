generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  //nomeColuna     tipo de dado
  id             String  @id @default(uuid())
  title          String
  details        String?
  slug           String  @unique
  maximumClients Int?    @map("maximus clients")

  @@map("events")
}
